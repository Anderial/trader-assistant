      "DeathVoteExpirationTimeoutFromMilliseconds": 120000,
      "TableRefreshTimeoutFromMilliseconds": 60000,
      "DefunctSiloCleanupPeriodFromMilliseconds": 60000,
      "DefunctSiloExpirationFromMilliseconds": 180000,
      "UseLivenessGossip": true,
      "GrainCollectionAgeFromMinutes": 10
    }
  },
  "SqsSettings": {
    "AccessKey": "",
    "SecretKey": "",
    "Region": "us-east-1",
    "ServiceUrl": ""
  },
  "Bybit": {
    "BaseUrl": "https://api.bybit.com",
    "TestnetUrl": "https://api-testnet.bybit.com"
  }
