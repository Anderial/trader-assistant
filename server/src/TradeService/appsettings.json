{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Orleans": "Information"
    }
  },
  "AllowedHosts": "*",
  "OrleansClusterSettings": {
    "ClusterId": "trader-assistant-cluster",
    "ServiceId": "trader-assistant-service",
    "SiloPort": 11111,
    "GatewayPort": 30000,
    "UseKubernetesHosting": false,
    "MongoMembership": {
      "ServiceUri": "localhost:27017",
      "Username": "root",
      "Password": "example",
      "ConnectTimeoutMS": 5000,
      "Tls": false,
      "DatabaseName": "OrleansClusterTraderAssistant",
      "Strategy": "SingleDocument"
